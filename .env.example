# Supabase Configuration
# Get these values from your Supabase project settings
VITE_SUPABASE_PROJECT_ID="your_project_id_here"
VITE_SUPABASE_PUBLISHABLE_KEY="your_anon_key_here"
VITE_SUPABASE_URL="https://your_project_id.supabase.co"

# Messenger360 API Configuration
# Get your API key from Messenger360 dashboard
VITE_MESSENGER360_API_KEY="your_messenger360_api_key_here"

# Webhook Security
# Generate a strong random secret for HMAC signature verification
# Example: openssl rand -hex 32
WEBHOOK_SECRET="your_webhook_secret_here"

# Sentry Configuration (Optional - for monitoring)
# Get your DSN from Sentry project settings
VITE_SENTRY_DSN="your_sentry_dsn_here"
SENTRY_AUTH_TOKEN="your_sentry_auth_token_here"

# Application Configuration
VITE_APP_DOMAIN="easyflow-pro.site"
VITE_APP_EMAIL="your_email@example.com"
